{
    "name": "monotickets/api",
    "type": "project",
    "description": "Monotickets API service with JWT authentication",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0",
        "tymon/jwt-auth": "^2.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
